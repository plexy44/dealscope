{
  "hosting": {
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "**",
        "run": {
          "serviceId": "dibsy-deals",
          "region": "europe-west4"
        }
      }
    ]
  },
  "apphosting": {
    "backendId": "dealscope",
    "rootDir": "src",
    "ignore": [
      "node_modules",
      ".git",
      "firebase-debug.log",
      "firebase-debug.*.log",
      "functions"
    ]
  }
}
